{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-rajnew-lab-datafactory"
		},
		"ADLSGen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLSGen2'"
		},
		"AzureBlobStorage1_in_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1_in'"
		},
		"AzureDataLakeStorage1_destgen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1_destgen2'"
		},
		"Imagedata_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Imagedata'"
		},
		"Imagedatals_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Imagedatals'"
		},
		"SQLDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SQLDB'"
		},
		"adfgithubgen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'adfgithubgen2'"
		},
		"datafilechicago_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'datafilechicago'"
		},
		"ADLSGen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsrajtorageaccount1.dfs.core.windows.net/"
		},
		"AzureDataLakeStorage1_destgen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://gen2storagelab.dfs.core.windows.net/"
		},
		"HttpServerchicago_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://data.cityofchicago.org/resource/crimes.json?$limit=50000"
		},
		"Httpcriminalrecordls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://data.cityofchicago.org/resource/crimes.json?$limit=50000"
		},
		"adfgithubgen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsrajtorageaccount1.dfs.core.windows.net/"
		},
		"criminalchicago_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://data.cityofchicago.org/resource/crimes.json?$limit=50000"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-rajnew-lab-datafactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADLSGen2_accountKey": {
						"value": "[parameters('ADLSGen2_accountKey')]"
					},
					"AzureBlobStorage1_in_connectionString": {
						"value": "[parameters('AzureBlobStorage1_in_connectionString')]"
					},
					"AzureDataLakeStorage1_destgen2_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_destgen2_accountKey')]"
					},
					"Imagedata_connectionString": {
						"value": "[parameters('Imagedata_connectionString')]"
					},
					"Imagedatals_connectionString": {
						"value": "[parameters('Imagedatals_connectionString')]"
					},
					"SQLDB_connectionString": {
						"value": "[parameters('SQLDB_connectionString')]"
					},
					"adfgithubgen2_accountKey": {
						"value": "[parameters('adfgithubgen2_accountKey')]"
					},
					"datafilechicago_connectionString": {
						"value": "[parameters('datafilechicago_connectionString')]"
					},
					"ADLSGen2_properties_typeProperties_url": {
						"value": "[parameters('ADLSGen2_properties_typeProperties_url')]"
					},
					"AzureDataLakeStorage1_destgen2_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_destgen2_properties_typeProperties_url')]"
					},
					"HttpServerchicago_properties_typeProperties_url": {
						"value": "[parameters('HttpServerchicago_properties_typeProperties_url')]"
					},
					"Httpcriminalrecordls_properties_typeProperties_url": {
						"value": "[parameters('Httpcriminalrecordls_properties_typeProperties_url')]"
					},
					"adfgithubgen2_properties_typeProperties_url": {
						"value": "[parameters('adfgithubgen2_properties_typeProperties_url')]"
					},
					"criminalchicago_properties_typeProperties_url": {
						"value": "[parameters('criminalchicago_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adf-rajnew-lab-datafactory_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-rajnew-lab-datafactory_ArmTemplate_0')]"
			]
		},
		{
			"name": "adf-rajnew-lab-datafactory_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-rajnew-lab-datafactory_ArmTemplate_1')]"
			]
		}
	]
}